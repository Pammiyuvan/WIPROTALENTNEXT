package wipro;


	
	public class day2{

public static void main (String[] args) {
    Scanner sc= new Scanner(System.in);
    System.out.print("Enter a value :");
    int a = sc.nextInt();
    System.out.print("Enter b value :");
    int b = sc.nextInt();
    
if (a>b)
    {
    System.out.print(a+"is greater than "+b);
    }
else {
    System.out.print(b+"is greater than "+a);
    }

}
	}




 
